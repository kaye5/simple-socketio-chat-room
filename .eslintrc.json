{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "extends": ["airbnb", "prettier", "plugin:react/recommended"],
    "plugins": ["prettier", "react"],
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-vars": "warn",
        "jsx-a11y/click-events-have-key-events": "off",
        "jsx-a11y/no-static-element-interactions": "off"
    }
}